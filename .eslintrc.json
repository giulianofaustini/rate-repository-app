{
    "env": {
      "browser": true,
      "es2021": true,
      "react-native/react-native": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module",
      "parser": "babel-eslint" 
    },
    "plugins": [
      "react",
      "react-native"
    ],
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "rules": {
      "react/prop-types": "off",
      "react/react-in-jsx-scope": "off"
    }
  }
  